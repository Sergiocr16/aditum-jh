<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity RevisionTask.
    -->
    <changeSet id="20200113194804-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="revision_id"
                                 baseTableName="revision_task"
                                 constraintName="fk_revision_task_revision_id"
                                 referencedColumnNames="id"
                                 referencedTableName="revision"/>

        <addForeignKeyConstraint baseColumnNames="revision_task_category_id"
                                 baseTableName="revision_task"
                                 constraintName="fk_revision_task_revision_task_category_id"
                                 referencedColumnNames="id"
                                 referencedTableName="revision_task_category"/>

    </changeSet>
</databaseChangeLog>
